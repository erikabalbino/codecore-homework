<%- include("../partials/header") %>

<form action="/teams" method="POST" >
  <div class="form-group">
    <label for="">Logo Url</label>
    <input
    class="form-control"
    style="color: gray"
    type="text"
    name="logoUrl"
    value=""
    placeholder="e.g. https://www.example.com/some.png"
    >
  </div>
  <div class="form-group">
    <label for="">Name</label>
    <input
    class="form-control"
    style="color: gray"
    type="text"
    name="name"
    value=""
    placeholder="Flying Funks"
    >
  </div>

  <div class="form-group">
    <label for="">Members</label>
    <textarea
    class="form-control"
    rows='5'
    name='members'
    value=""
    placeholder="Erika, Paula, Maria, Ana"
    ></textarea>
  </div>
  <input
  class="btn btn-outline-primary"
  type='submit'
  value='Submit'
  />
</form>

<%- include("../partials/footer") %>
